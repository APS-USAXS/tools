					<PV label="trans_pin_gain" pvname="15iddLAX:USAXS:TR_pinGain" />
					<PV label="trans_I0_counts" pvname="15iddLAX:USAXS:TR_I0Counts" />
					<PV label="trans_I0_gain" pvname="15iddLAX:USAXS:TR_I0Gain" />

					<PV label="hhl_slitb" pvname="15IDA:m20.RBV" />
					<PV label="hhl_slitl" pvname="15IDA:m19.RBV" />
					<PV label="hhl_slitr" pvname="15IDA:m17.RBV" />
					<PV label="hhl_slitt" pvname="15IDA:m18.RBV" />

					<!-- I0 amplifier gains at start of scan and gains and measured backgrounds -->
					<PV label="I0AmpGain"    pvname="15iddUSX:fem02:seq01:gain" />

					<PV label="I0_gain0"     pvname="15iddUSX:pd02:seq01:gain0" />
					<PV label="I0_bkg0"      pvname="15iddUSX:pd02:seq01:bkg0" />
					<PV label="I0_bkg_err0"  pvname="15iddUSX:pd02:seq01:bkgErr0" />

					<PV label="I0_gain1"     pvname="15iddUSX:pd02:seq01:gain1" />
					<PV label="I0_bkg1"      pvname="15iddUSX:pd02:seq01:bkg1" />
					<PV label="I0_bkg_err1"  pvname="15iddUSX:pd02:seq01:bkgErr1" />

					<PV label="I0_gain2"     pvname="15iddUSX:pd02:seq01:gain2" />
					<PV label="I0_bkg2"      pvname="15iddUSX:pd02:seq01:bkg2" />
					<PV label="I0_bkg_err2"  pvname="15iddUSX:pd02:seq01:bkgErr2" />

					<PV label="I0_gain3"     pvname="15iddUSX:pd02:seq01:gain3" />
					<PV label="I0_bkg3"      pvname="15iddUSX:pd02:seq01:bkg3" />
