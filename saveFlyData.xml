<?xml version="1.0"?>


<!-- 

You MUST set EPICS_CA_MAX_ARRAY_BYTES to >= 1280000 to read the maximum
number of data points the MCS can be told to acquire (15iddLAX:3820:MaxChannels).

This is programmed into the Python code that reads this file.

After editing, confirm this file is valid with this command::

  xmllint -/-schema saveFlyData.xsd saveFlyData.xml

OR, check this file for validity and that all PVs exist::

  EPD check_saveFlyData.py

 -->
 
 
<saveFlyData version="1">
	<triggerPV pvname="15iddLAX:USAXSfly:Start" start_value="1"
		start_text="Busy" done_value="0" done_text="Done" />

	<NX_structure>  <!-- http://download.nexusformat.org/doc/html/classes/base_classes/ -->
		<group name="/" class="file">
			<attribute name="instrument" value="APS USAXS at 15ID-D" />
			<attribute name="scan_mode" value="USAXS fly scan" />

			<group name="entry" class="NXentry">
				<field name="program_name">
					<!-- version of this configuration file -->
					<attribute name="config_version" value="1.1" />
					<text>saveFlyData.py</text>
				</field>
				<PV label="title" pvname="15iddLAX:USAXS:sampleTitle" />
				<PV label="experiment_identifier" pvname="15iddLAX:GUPNumber" />
				<PV label="run_cycle" pvname="15iddLAX:RunCycle" />

				<group name="instrument" class="NXinstrument">

					<field name="name">
						<attribute name="short_name" value="USAXS" />
						<text>APS USAXS at 15ID-D</text>
					</field>
					<group name="monochromator" class="NXmonochromator">
						<PV label="energy" pvname="15IDA:BraggERdbkAO" />
						<PV label="wavelength" pvname="15IDA:BraggLambdaRdbkAO" />
					</group>
				</group>

				<group name="user" class="NXuser">
					<PV label="name" pvname="15iddLAX:UserName" />
				</group>

				<group name="sample" class="NXsample">
					<PV label="name" pvname="15iddLAX:USAXS:sampleTitle" />
					<PV label="thickness" pvname="15iddLAX:USAXS:SampleThickness" />
				</group>

				<group name="flyScan" class="NXdata">

					<!-- positioners -->
					<PV label="AR_PulsePositions" pvname="15iddLAX:traj1:PulsePositions">
						<attribute name="units" value="degrees" />
					</PV>
					<PV label="AR_NumPulsePositions" pvname="15iddLAX:traj1:NumPulsePositions" />
					<PV label="AR_PulseMode" pvname="15iddLAX:traj1:PulseMode" />
					<!--
			   PulseMode is string with type of scan:
			   1. Fixed fixed pulses as before (we should have that nailed down by now already)
			   2. Array use PulsePositions
			   3. TrajPts use trajectory points (the 200 points in current trajectory)
			-->

					<PV label="AR_waypoints" pvname="15iddLAX:traj1:M1Traj"
						length_limit="AR_count" />
					<PV label="AR_count" pvname="15iddLAX:traj1:Nelements" />
					<PV label="AR_pulses" pvname="15iddLAX:traj1:Npulses" />
					<PV label="AR_start" pvname="15iddLAX:USAXS:ARstart" />  <!-- pushed here by SPEC usaxs_flyscan.txt macro -->
					<PV label="AR_increment" pvname="15iddLAX:USAXS:MinStep" />
					<PV label="AR_MinStepTraj" pvname="15iddLAX:USAXS:MinStepTraj" /> <!-- minimum step of trajectory waypoints -->
					<PV label="AR_Finish" pvname="15iddLAX:USAXS:Finish" />
					<PV label="StartOffset" pvname="15iddLAX:USAXS:StartOffset" />

					<PV label="time_total" pvname="15iddLAX:traj1:ElapsedTime" />
					<PV label="time_elapsed" pvname="15iddLAX:traj1:Time" />


					<!-- detectors -->
					<PV label="mca_clock_frequency" pvname="15iddLAX:3820:clock_frequency" />
					<PV label="mca_channels" pvname="15iddLAX:3820:CurrentChannel" />
					<PV label="mca_channel_max" pvname="15iddLAX:3820:MaxChannels" />
					<PV label="mca_channels_used" pvname="15iddLAX:3820:NuseAll">
						<attribute name="meaning" value="number of Struck channels to use" />
					</PV>

					<PV label="mca1_name" pvname="15iddLAX:3820:scaler1.NM1" />
					<PV label="mca1" pvname="15iddLAX:3820:mca1" length_limit="mca_channels">		<!-- counts of 50 MHz clock -->
						<attribute name="units" value="pulses" />
						<attribute name="USAXS_name" value="clock_pulses" />
					</PV>

					<PV label="mca2_name" pvname="15iddLAX:3820:scaler1.NM2" />
					<PV label="mca2" pvname="15iddLAX:3820:mca2" length_limit="mca_channels">
						<attribute name="units" value="counts" />
						<attribute name="USAXS_name" value="I0" />
					</PV>

					<PV label="mca3_name" pvname="15iddLAX:3820:scaler1.NM3" />
					<PV label="mca3" pvname="15iddLAX:3820:mca3" length_limit="mca_channels">
						<attribute name="signal" value="1" />
						<!-- <attribute name="axes" value="AR" /> -->
						<!-- The proper ordinate is obtained via the MAC channel index: 
							AR[i] = AR_start + AR_increment * i 
							
							Can the AR array be made available in EPICS? 
						-->
						<attribute name="units" value="counts" />
						<attribute name="USAXS_name" value="upd" />
					</PV>

					<!-- 2014-02-28, PRJ
					    The following PVs record amplifier gain changes 
						and the corresponding MCS channel number at which the change occurred
						for each of four detector amplifiers.  (Two monitors and two UPD amplifiers)
						Choice of the UPD amplifier in use is found in: 15iddUSX:femto:model
						which takes either of these two values:
						* 0: "DLPCA200"
						* 1: "DDPCA300"
						-->

					<PV label="upd_flyScan_amplifier"    pvname="15iddUSX:femto:model" />	<!--saves as number, not string-->
					<PV label="upd_flyScan_amplifier_ZNAM"    pvname="15iddUSX:femto:model.ZNAM" />
					<PV label="upd_flyScan_amplifier_ONAM"    pvname="15iddUSX:femto:model.ONAM" />

					<PV label="changes_I0_mcsChan"       pvname="15iddLAX:USAXSfly:I0:mcsChan" />
					<PV label="changes_I0_ampGain"       pvname="15iddLAX:USAXSfly:I0:ampGain" />
					<PV label="changes_I0_ampReqGain"    pvname="15iddLAX:USAXSfly:I0:ampReqGain" />

					<PV label="changes_I00_mcsChan"      pvname="15iddLAX:USAXSfly:I00:mcsChan" />
					<PV label="changes_I00_ampGain"      pvname="15iddLAX:USAXSfly:I00:ampGain" />
					<PV label="changes_I00_ampReqGain"   pvname="15iddLAX:USAXSfly:I00:ampReqGain" />

					<PV label="changes_DLPCA200_mcsChan"    pvname="15iddLAX:USAXSfly:DLPCA200:mcsChan" />
					<PV label="changes_DLPCA200_ampGain"    pvname="15iddLAX:USAXSfly:DLPCA200:ampGain" />
					<PV label="changes_DLPCA200_ampReqGain" pvname="15iddLAX:USAXSfly:DLPCA200:ampReqGain" />

					<PV label="changes_DDPCA300_mcsChan"    pvname="15iddLAX:USAXSfly:DDPCA300:mcsChan" />
					<PV label="changes_DDPCA300_ampGain"    pvname="15iddLAX:USAXSfly:DDPCA300:ampGain" />
					<PV label="changes_DDPCA300_ampReqGain" pvname="15iddLAX:USAXSfly:DDPCA300:ampReqGain" />

					<PV label="AYDYstart" pvname="15iddLAX:USAXSfly:AYDYstart">
						<attribute name="meaning"
							value="point number when to start moving ay and dy stages" />
					</PV>
					<PV label="dyMaxSpeed" pvname="15iddLAX:USAXSfly:dyMaxSpeed" />
					<PV label="ayMaxSpeed" pvname="15iddLAX:USAXSfly:ayMaxSpeed" />
					<PV label="arMaxSpeed" pvname="15iddLAX:USAXSfly:arMaxSpeed" />
					<!-- do not save these three PVs, they are not quite the same as the three above 
						<PV label="FS_AyMaxSpeed" pvname="15iddLAX:USAXS:FS_AyMaxSpeed" /> 
						<PV label="FS_DyMaxSpeed" pvname="15iddLAX:USAXS:FS_DyMaxSpeed" /> 
						<PV label="FS_ArMaxSpeed" pvname="15iddLAX:USAXS:FS_ArMaxSpeed" /> 
					-->
					<PV label="FS_ElapsedTime" pvname="15iddLAX:USAXS:FS_ElapsedTime" />

					<PV label="FS_NumberOfPoints" pvname="15iddLAX:USAXS:FS_NumberOfPoints">
						<attribute name="meaning" value="number of waypoints in trajectory scan" />
					</PV>
					<PV label="FS_ScanTime" pvname="15iddLAX:USAXS:FS_ScanTime" />
					<PV label="FS_ArStartAyDyMove" pvname="15iddLAX:USAXS:FS_ArStartAyDyMove" />

					<PV label="changes_AR_PSOpulse"    pvname="15iddLAX:USAXSfly:AR:mcsChan" />
					<PV label="changes_AR_angle"      pvname="15iddLAX:USAXSfly:AR:pos" >
						<attribute name="meaning"
							value="AR readback when PSO pulse fired with 10Hz jitter" />
					</PV>

    					<PV label="is_2D_USAXS_scan"    pvname="15iddLAX:USAXS:is2DUSAXSscan" >
						<attribute name="meaning"
							value="does this scan use 2D collimated geometry?" />
					</PV>

				</group> 	<!-- entry:NXentry/flyScan:NXdata -->


				<!-- metadata -->
				<group name="metadata" class="NXcollection">    <!-- metadata:NXcollection -->

					<!-- beam line conditions -->
					<PV label="SR_current" pvname="S:SRcurrentAI" />
					<PV label="ID_E" pvname="ID15:Energy" />
					<PV label="DCM_energy" pvname="15IDA:BraggERdbkAO" />
					<PV label="DCM_wavelength" pvname="15IDA:BraggLambdaRdbkAO" />
					<PV label="DCM_theta" pvname="15IDA:m10.RBV" />
