					<!-- SPEC -->
					<PV label="SPEC_macro_file_string19" pvname="15iddLAX:string19" />
					<PV label="SPEC_macro_file" pvname="15iddLAX:USAXS:macroFile" />
					<PV label="SPEC_data_file" pvname="15iddLAX:USAXS:specFile" />
					<PV label="SPEC_data_dir" pvname="15iddLAX:USAXS:userDir" />
